<!DOCTYPE html>
<html lang="en">

<head>

    <title>Register</title>
    
    <!-- <link rel="stylesheet" href="./../../public/stylesheets/dashboard.css"> -->
    <%- include ('../partials/libs') %>

</head>

<body class="bg-gradient-primary">

    <div class="container">

        <!-- Outer Row -->
        <div class="row justify-content-center">

            <div class="col-xl-6 col-lg-6 col-md-9">
                <div class="card o-hidden border-0 shadow-lg my-5">
                    <div class="card-body p-0">
                        <!-- Nested Row within Card Body -->
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="p-5">
                                    <div class="text-center">
                                        <h1 class="h4 text-gray-900 mb-4">Register result</h1>
                                    </div>
                                    <% if(error_flag) { %>
                                    <div class="alert alert-danger text-danger text-center small">
                                        <%= message %>
                                    </div>
                                    <% } %>
                                    <% if(success_flag) { %>
                                        <div class="alert alert-success text-success text-center small">
                                            <%= message %>
                                        </div>
                                    <% } %>
                                    <a href="../dashboard" class="btn btn-primary btn-user btn-block">
                                            Continue
                                        </a>
                                    <hr>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

</body>

</html>